ge.getItem(\"langNow\"));\n\n    const [showMenu, setShowMenu] = useState(false);\n    const [activeItem, setActiveItem] = useState('home');\n\n    const bgImage = { img : require('../MainAboutTop/mainAboutTopBg.png') }\n\n    const navigate = useNavigate();\n    const handleAlbumClick = (page) => {\n        navigate(`/${page}`);\n        setActiveItem(page);\n        setShowMenu(false);\n    };\n\n    const toggleLanguage = (lang) => {\n        setLangOn(lang)\n        changeLanguage(lang);\n        console.log('toggleLang  ' + toggleLang)\n        console.log('lang  ' + lang)\n    };\n\n    const menuToggler = () => {\n        setShowMenu(!showMenu);\n    }\n\n    const headerLogo = {\n        desk : require('../../images/logo.png'),\n        mob : require('../../images/logo_mob.png')\n    }\n\n    const [isMobileLogo , setIsMobileLogo] = useState(false)\n\n    useEffect(() => {\n        console.log(headerLogo)\n        const mediaQuery = window.matchMedia('(max-width: 480px)'); // Установите нужный вам медиа-запрос\n        const handleResize = (event) => {\n            setIsMobileLogo(event.matches);\n        };\n        mediaQuery.addListener(handleResize);\n        setIsMobileLogo(mediaQuery.matches);\n        return () => {\n            mediaQuery.removeListener(handleResize);\n        };\n\n    }, []);\n\n\n\n\n    return(\n            <div className='header'>\n                <div className='logo'>\n                    { isMobileLogo ?\n                        (\n                            <img onClick={() => handleAlbumClick('')} src={headerLogo.mob} alt='Belmar'/>\n                        )\n                        :\n                        (\n                            <img onClick={() => handleAlbumClick('')} src={headerLogo.desk} alt='Belmar'/>\n                        )\n                    }\n                    {/*<h1 onClick={() => handleAlbumClick('')}>Belmar</h1>*/}\n                    {/*<span onClick={() => handleAlbumClick('')}>marketing</span>*/}\n                </div>\n                <div className='navbar_links'>\n                    <div className=\"navbar_links_container\">\n                        <button className={classnames('navbar_links_container-item', { 'active': activeItem === 'about_us' })} onClick={() => handleAlbumClick('about_us')}>{translations.navBarItems[0]}</button>\n                        <button className={classnames('navbar_links_container-item', { 'active': activeItem === 'events' })} onClick={() => handleAlbumClick('events')}>{translations.navBarItems[1]}</button>\n                        <button className={classnames('navbar_links_container-item', { 'active': activeItem === 'contacts' })} onClick={() => handleAlbumClick('contacts')}>{translations.navBarItems[2]}</button>\n                    </div>\n                </div>\n                <div className='lang'>\n                    <div className='navbar_link_mob'>\n                        <img onClick={menuToggler} src={menuBtn} alt=\"\"/>\n                    </div>\n                    {/*<h1 onClick={toggleLanguage}>{`>${translations.toggleLang}`}</h1>*/}\n                    <h1><span className={langOn == 'en' ? 'active_lang' : ''} onClick={() => toggleLanguage('en')}>en</span> | <span className={langOn == 'ua' ? 'active_lang' : ''} onClick={() => toggleLanguage('ua')}>ua</span></h1>\n                </div>\n                {\n                    showMenu ? (\n                        <div className=\"mob_menu\" style={{backgroundImage: `url(\"${bgImage.img}\")`}}>\n                            <div className='mob_menu-close'>\n                                <img onClick={menuToggler} src={mobMenuClose} alt=\"\"/>\n                            </div>\n                            <div className=\"mob_menu_links_container\">\n                                <button className={classnames('mob_menu_links_container-item', { 'active': activeItem === 'about_us' })} onClick={() => handleAlbumClick('about_us')}>{translations.navBarItems[0]}</button>\n                                <button className={classnames('mob_menu_links_container-item', { 'active': activeItem === 'events' })} onClick={() => handleAlbumClick('events')}>{translations.navBarItems[1]}</button>\n                                <button className={classnames('mob_menu_links_container-item', { 'active': activeItem === 'contacts' })} onClick={() => handleAlbumClick('contacts')}>{translations.navBarItems[2]}</button>\n                            </div>\n                        </div>) : false\n                }\n            </div>\n\n    )\n}","/Applications/MAMP/htdocs/Belmar_land/src/components/MainAbout/MainAbout.jsx",[],[],"/Applications/MAMP/htdocs/Belmar_land/src/components/Hedader/Hedader.jsx",[],[],"/Applications/MAMP/htdocs/Belmar_land/src/components/MainAboutBottom/MainAboutBottom.jsx",[],[],"/Applications/MAMP/htdocs/Belmar_land/src/components/MainAboutTop/MainAboutTop.jsx",[],[],"/Applications/MAMP/htdocs/Belmar_land/src/components/MainAboutPartners/MainAboutPartners.jsx",["203"],[],"import comp_logo_1 from '../../images/CompanyLogo-4.png';\nimport comp_logo_2 from '../../images/CompanyLogo.png';\nimport comp_logo_3 from '../../images/CompanyLogo-3.png';\nimport comp_logo_4 from '../../images/CompanyLogo-5.png';\nimport comp_logo_5 from '../../images/CompanyLogo-2.png';\nimport './mainAboutPartners.css';\nimport OverflowScrolling from 'react-overflow-scrolling';\nimport LanguageContext from \"../../LanguageContext/LanguageContext\";\nimport {useContext, useEffect, useState} from \"react\";\nexport default function MainAboutPartners({isMobileTitle}){\n    const { translations } = useContext(LanguageContext);\n\n    const [activeIndex, setActiveIndex] = useState(0);\n    const imgSourceArr = [\n        {comp_logo : require('../../images/CompanyLogo-4.png')},\n        {comp_logo : require('../../images/CompanyLogo.png')},\n        {comp_logo : require('../../images/CompanyLogo-3.png')},\n        {comp_logo : require('../../images/CompanyLogo-5.png')},\n        {comp_logo : require('../../images/CompanyLogo-2.png')}\n    ]\n\n    useEffect(() => {\n        const interval = setInterval(() => {\n            setActiveIndex((prevIndex) => (prevIndex + 1) % imgSourceArr.length);\n        }, 1000);\n\n        return () => clearInterval(interval);\n    }, []);\n\n    return(\n        <div className='main_about_partners'>\n            <div className='main_about_partners_title'>\n                <h1>\n                    {translations.mainPagePartnersTitle}\n                </h1>\n            </div>\n            {isMobileTitle ?\n                (<OverflowScrolling className='overflow-scrolling'>\n                    <div className='main_about_partners_list_item'><img src={comp_logo_1} alt=\"1\"/></div>\n                    <div className='main_about_partners_list_item'><img src={comp_logo_2} alt=\"2\"/></div>\n                    <div className='main_about_partners_list_item'><img src={comp_logo_3} alt=\"3\"/></div>\n                    <div className='main_about_partners_list_item'><img src={comp_logo_4} alt=\"4\"/></div>\n                    <div className='main_about_partners_list_item'><img src={comp_logo_5} alt=\"5\"/></div>\n                </OverflowScrolling>)\n                    :\n                (<div className='main_about_partners_list'>\n                    {\n                        imgSourceArr.map((e,i)=>{\n                            // console.log(i)\n                            return(\n                                <div key={i} className={i === activeIndex ? 'main_about_partners_list_item active' : 'main_about_partners_list_item'}><img src={e.comp_logo} alt=\"company logo\"/></div>\n                            )\n                        })\n                    }\n                </div>)\n            }\n        </div>\n    )\n}","/Applications/MAMP/htdocs/Belmar_land/src/components/HedaderTop/HedaderTop.jsx",[],[],"/Applications/MAMP/htdocs/Belmar_land/src/components/HedaderToggSide/HedaderToggSide.jsx",["204","205"],[],"import {useEffect, useState} from \"react\";\nimport './hedader_togg_side.css';\nimport LanguageContext from \"../../LanguageContext/LanguageContext\";\nimport {useContext} from \"react\";\n\nexport default function HedaderToggSide(){\n\n    const { changeAffAdv } = useContext(LanguageContext);\n    const [isToggled, toggle] = useState(false); // aff ==> false , adv ==> true\n\n    useEffect(() => {\n\n        setTimeout(()=>{\n            const affOrAdvNow = localStorage.getItem(\"affOrAdvNow\");\n            (affOrAdvNow == 'adv') ? toggle(true) : toggle(false);\n            console.log('isToggled hedader',isToggled, affOrAdvNow)\n        },1500)\n\n    },[])\n\n    const callback = () => {\n        toggle(!isToggled);\n        sendToContext(!isToggled);\n    }\n\n    const handleChange = () => {\n        console.log('handleChange',isToggled)\n    }\n\n    const sendToContext = (val) => {\n        val ? changeAffAdv('adv', true) : changeAffAdv('aff', false);\n        console.log('toggle', isToggled, val)\n    }\n\n    return (\n        <div className='hedader_togg_side'>\n            <div className='hedader_togg_side_rot'>\n                <div className='aff_side'>\n                    <h3 className='hedader_togg_side_tittle'>Affiliate</h3>\n                </div>\n                <div>\n                    <label className='hedader_togg_side_label'>\n                        <input className='hedader_togg_side_input' type=\"checkbox\" onChange={handleChange} checked={isToggled} onClick={callback} />\n                        <span className='hedader_togg_side_span' />\n\n                    </label>\n                </div>\n                <div className='adv_side'>\n                    <h3 className='hedader_togg_side_tittle'>Advertiser</h3>\n                </div>\n            </div>\n        </div>\n    )\n}","/Applications/MAMP/htdocs/Belmar_land/src/components/HedaderBottom/HedaderBottom.jsx",[],[],"