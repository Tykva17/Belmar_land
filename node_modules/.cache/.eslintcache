gNow , 'langNow if 1');\n        }else{\n            setToggleLang(langNow);\n            console.log(langNow , 'langNow if 2');\n\n        }\n        console.log(langNow , 'langNow if after');\n\n\n        const affOrAdvNow = localStorage.getItem(\"affOrAdvNow\");\n        if(affOrAdvNow !== undefined || affOrAdvNow !== null){\n            localStorage.setItem(\"affOrAdvNow\", \"aff\");\n            setAffAdv('aff');\n        }else{\n            setAffAdv(affOrAdvNow);\n        }\n\n        console.log(\"toggleLang : \" , toggleLang);\n        console.log(\"affAdv : \" , affAdv);\n    },[setToggleLang, setAffAdv]);\n\n\n\n\n    return (\n        <LanguageContext.Provider value={{ translations, changeLanguage, changeAffAdv }}>\n            {children}\n        </LanguageContext.Provider>\n    );\n};\nexport default LanguageContext;\n",{"ruleId":"176","severity":1,"message":"177","line":1,"column":9,"nodeType":"178","messageId":"179","endLine":1,"endColumn":18},{"ruleId":"180","severity":1,"message":"181","line":15,"column":17,"nodeType":"182","messageId":"183","endLine":15,"endColumn":19},{"ruleId":"180","severity":1,"message":"181","line":15,"column":35,"nodeType":"182","messageId":"183","endLine":15,"endColumn":37},{"ruleId":"180","severity":1,"message":"181","line":17,"column":23,"nodeType":"182","messageId":"183","endLine":17,"endColumn":25},{"ruleId":"180","severity":1,"message":"181","line":17,"column":41,"nodeType":"182","messageId":"183","endLine":17,"endColumn":43},{"ruleId":"180","severity":1,"message":"181","line":19,"column":23,"nodeType":"182","messageId":"183","endLine":19,"endColumn":25},{"ruleId":"180","severity":1,"message":"181","line":19,"column":41,"nodeType":"182","messageId":"183","endLine":19,"endColumn":43},{"ruleId":"180","severity":1,"message":"181","line":21,"column":23,"nodeType":"182","messageId":"183","endLine":21,"endColumn":25},{"ruleId":"180","severity":1,"message":"181","line":21,"column":41,"nodeType":"182","messageId":"183","endLin